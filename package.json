    "test:GiftAPI" : "./node_modules/artillery/bin/artillery run -o ./Report/GiftReport.json ./Test/GiftApiTest.yml",
    "report:GiftAPI" : "./node_modules/artillery/bin/artillery report ./Report/GiftReport.json"
  },
